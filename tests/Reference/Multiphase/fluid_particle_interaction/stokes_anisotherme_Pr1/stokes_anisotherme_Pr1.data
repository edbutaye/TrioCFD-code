}

VDF dis

Schema_Euler_explicite sch
Lire sch
{
	tinit 0.
	tmax 1e-3
 	# nb_pas_dt_max 10 #
        dt_min 4e-11
	dt_max 1e-6
	dt_start dt_min

	diffusion_implicite 1
    	seuil_diffusion_implicite 1e-11
    	impr_diffusion_implicite 1
	dt_impr 5e-6
	dt_impr_fpi 5e-6
	dt_sauv 100
	seuil_statio -1

}
system "mkdir -p lata"
system "rm -f *.txt"

Particule_Solide particule_solide
Lire particule_solide
{

	mu  Champ_Uniforme 	1 3e0
 	rho Champ_Uniforme 	1 1e4
    	e_dry 			  0.97
       	lambda champ_uniforme 	1 1
    	cp champ_uniforme 	1 1300
	monodisperse 1
	rayon_frottements 2.6e-5 rayon_collision 2.6e-5
}

Fluide_Incompressible fluide
Lire fluide
{
	mu  Champ_Uniforme 	1 3e-3
 	rho Champ_Uniforme 	1 1e3
 	lambda champ_uniforme 	1 0.5
 	cp champ_uniforme 	1 333
}

Fluide_Diphasique fluide_diph
Lire fluide_diph
{
	fluide0 particule_solide
	fluide1 fluide
	sigma   Champ_Uniforme 1 0
  	formule_mu harmonique
  	chaleur_latente Champ_Uniforme 1    -1e10
}

Champ_Uniforme gravite
Lire gravite 3 0. 0.  -10
Associate fluide_diph gravite


Navier_Stokes_FT_Disc              		hydraulique
Convection_Diffusion_Temperature_FT_Disc 	thermique 
Transport_Interfaces_FT_Disc       		interf

Associate pb hydraulique
Associate pb thermique
Associate pb interf

Associate pb dom
Associate pb sch
Associate pb fluide_diph

